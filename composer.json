{
  "name": "fliq/ipfs-laravel",
  "description": ":package_description",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Pav",
      "email": "_@christians.email",
      "homepage": "http://fliqnft.com"
    }
  ],
  "homepage": "https://github.com/fliq/ipfs-laravel",
  "keywords": [
    "Laravel",
    "IpfsLaravel",
    "Web3"
  ],
  "require": {
    "illuminate/support": "~9",
    "fliq/ipfs": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~7",
    "pestphp/pest": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "Fliq\\IpfsLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fliq\\IpfsLaravel\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fliq\\IpfsLaravel\\IpfsServiceProvider"
      ],
      "aliases": {
        "Ipfs": "Fliq\\IpfsLaravel\\Facades\\Ipfs"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
